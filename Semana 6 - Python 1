# Exemplos de tipos de dados
numero = 10
nome = "João"
booleano = True

# Exemplos - Operações Aritméticas
a = 10
b = 5
soma = a + b
subtracao = a - b
multiplicacao = a * b
divisao = a / b

# Operações Lógicas e de Comparação
a = True
b = False
resultado1 = a and b
resultado2 = a or b
resultado3 = not a

c = 3
c == 4
c > 2
c != 3

d = 4
c is d
c is not d

# Ordem de precedência dos operadores lógicos
# A ordem de precedência dos operadores lógicos é: not > and > or

# Modificar a ordem das operações lógicas usando parênteses
# Exemplo:
resultado4 = not (a and b or c > 2)
resultado5 = (not a) and (b or c > 2)

